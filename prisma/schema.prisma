datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model MedicalText {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  text           String
  task           String
  confidence     Float
  annotateTime   Int      @default(0)
}
